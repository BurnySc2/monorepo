{
    "name": "my-app",
    "version": "0.0.1",
    "scripts": {
        "dev": "cross-env BACKEND_SERVER='localhost:8000' vite dev --port 3000 --host",
        "build": "vite build",
        "build-precommit": "vite build | grep ERR",
        "preview": "vite preview",
        "preview2": "cross-env BACKEND_SERVER='localhost:7899' npm run build && cross-env BACKEND_SERVER='localhost:7899' vite preview -p 3000",
        "test": "playwright test",
        "test:all": "playwright test --browser=all --workers=1",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "eslint --max-warnings 0 .",
        "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
        "format:check": "prettier --check --ignore-path .gitignore --write --plugin-search-dir=. .",
        "prune": "rm node_modules -rf && rm package-lock.json -f && npm i",
        "update": "npx npm-check-updates -u && npm run prune"
    },
    "devDependencies": {
        "@playwright/test": "^1.30.0",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "prettier": "^2.8.4",
        "prettier-plugin-svelte": "^2.9.0"
    },
    "dependencies": {
        "@sveltejs/adapter-netlify": "^2.0.5",
        "@sveltejs/adapter-static": "^2.0.1",
        "@sveltejs/kit": "1.7.2",
        "moment": "^2.29.4",
        "svelte": "^3.55.1",
        "svelte-check": "^3.0.3",
        "svelte-preprocess": "^5.0.1",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5",
        "vite": "^4.0.0",
        "vitest": "^0.25.3"
    },
    "type": "module"
}
