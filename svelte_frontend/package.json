{
    "name": "my-app",
    "version": "0.0.1",
    "scripts": {
        "dev": "cross-env BACKEND_SERVER='localhost:8000' vite dev --port 3000",
        "build": "vite build",
        "build-precommit": "vite build | grep ERR",
        "preview": "vite preview",
        "preview2": "cross-env BACKEND_SERVER='localhost:7899' npm run build && cross-env BACKEND_SERVER='localhost:7899' vite preview -p 3000",
        "test": "playwright test",
        "test:all": "playwright test --browser=all --workers=1",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "eslint --ignore-path .gitignore .  --max-warnings 0",
        "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
        "prune": "rm node_modules -rf && rm package-lock.json -f && npm i",
        "update": "npx npm-check-updates -u && npm run prune"
    },
    "devDependencies": {
        "@playwright/test": "^1.25.1",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "prettier": "^2.7.1",
        "prettier-plugin-svelte": "^2.7.0"
    },
    "dependencies": {
        "@sveltejs/adapter-auto": "1.0.0-next.71",
        "@sveltejs/kit": "1.0.0-next.377",
        "moment": "^2.29.4",
        "svelte": "^3.50.0",
        "svelte-check": "^2.9.0",
        "svelte-preprocess": "^4.10.7",
        "typescript": "^4.8.2"
    },
    "type": "module"
}
